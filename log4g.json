{
  "level": "trace",
  "prefix": "[log4g]",
  "flag": "date|microseconds|shortfile",
  "loggers":[
    {
      "prefix": "[stdout]",
      "flag": "date|microseconds|longfile",
      "output": "stdout"
    },
    {
      "output": "stderr"
    },
    {
      "output": "file",
      "filename": "logs/debug.log",
      "maxsize": 0,
      "maxlines": 10,
      "daily": false
    },
    {
      "disabled": true,
      "output": "socket"
    },
    {
      "disabled": true,
      "output": "elasticsearch"
    }
  ]
}